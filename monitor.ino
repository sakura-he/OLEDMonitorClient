Ticker isCON;                 // 检测连接定时器
Ticker gifTicker;             // 更新loading gif 数组索引定时器
const char *SSID = "iPhone"; // WiFi名 SSID
const char *SSIDPW = "12121212";      // WiFi密码,没有留空 SSISPASSWORD
byte CONStatusCode = 0;
byte CONTimer = 0; // 连接计时器
byte gifIndex = 0; // loading gif帧数组索引
bool loadGIndxFstTimer = 1;
void setup()
{
    pinMode(LED_BUILTIN, OUTPUT); // 不需要led灯可以注释掉这一行 no need led pleas
    Serial.begin(9600);
    WiFi.mode(WIFI_STA);
    WiFi.begin(SSID, SSIDPW);
    u8g2.begin();
    isCON.once_ms(200, isCONFun); // 定时检测连接状态
}
void loop()
{
    if (CONStatusCode < 2) // 未连接服务器,判断各级连接是否正常
    {
        switch (CONStatusCode)
        {
        case 0:
            if (CONTimer >= 50) // 10 秒超时重连
            {
                WiFi.printDiag(Serial);
                Serial.println("WiFi未连接,尝试重新连接");
                WiFi.disconnect();
                WiFi.begin(SSID, SSIDPW); // 尝试重新连接WiFi
                CONTimer = 0;
            };
            break;
        }
    }
    OLEDDraw();
}
// 定时检测客户端和服务器的连接状态
void isCONFun()
{
    Serial.println("CONStatusCode is:");
    Serial.print(WiFi.status());
    if (WiFi.status() != WL_CONNECTED) // WiFi未连接
    {
        CONStatusCode = 0; // 降级连接状态检测码
        CONTimer++;        // 开启重连WiFi定时器
        isCON.once_ms(200, isCONFun);
        return;
    }
    else
    {
        isCON.once_ms(200, isCONFun);  // 临时测试 ,因该在下面的内容
        if (CONStatusCode < 1) // WiFi已连接,但状态码为未连接WiFi值
            CONStatusCode = 1; // 更新状态码
        if (CONTimer != 0)
            CONTimer = 0; // 重置连接计时器码
    }
}
// 根据状态码来更新屏幕显示内容
void OLEDDraw()
{
    switch (CONStatusCode)
    {
    case 0: // WiFi未连接显式内容
        //开启一个定时器更新索引,没有更新状态码只能执行一次"开启帧定时器",除非状态码更新,但状态码更新要确保上次状态码的帧处理定时器关闭
        if (loadGIndxFstTimer) // 开启gif索引定时器
        {
            gifTicker.once_ms(30, gifIndexSw);
            loadGIndxFstTimer = 0; // 关闭
        }
        u8g2.clearBuffer();
        u8g2.drawXBMP(0 + 64 + 12, 0, 64, 64, gifList[gifIndex]);
        u8g2.setFont(u8g2_font_helvB12_te);
        u8g2.drawXBMP(0, 0, 32, 32, wifi);
        u8g2.drawStr(64 - 64, 64 - 16, "WiFi");
        u8g2.setFont(u8g2_font_9x18_tf);
        u8g2.drawStr(64 - 64, 64, "Connection");
        u8g2.sendBuffer();
        break;
    case 1:
        u8g2.clearBuffer();
        u8g2.drawStr(0, 20, "HelloWorld");
        u8g2.sendBuffer();
    }
}

