void getServaerInfo()
{
    if (CONStatusCode < 2) // 没连接到服务端作甚?
        return;
    client.write("get");    // 请求数据
    Tcp_Handler(readTcp()); // 数据处理
}
// 获取tcp服务端数据
void Tcp_Handler(String data)
{
    String tempStr = "";
    //Serial.println(data);
    if (!(data.charAt(0) == '?' && data.charAt(data.length() - 1) == '!')) // 字符不是?-!形式
        ServerLoading = 1;                                                 // 服务器初始化中
    else
    {
        ServerLoading = 0; // 关闭加载动画
        // 去掉首尾字符
        data = (data.substring(1, data.length() - 1)).substring(0, data.length() - 2);
        // 处理获取到的字符到信息数组中infoArr

        for (int i = 0; i < 3; i++) // 遍历字符串
        {
            tempStr = (getValue(data, '-', i));
            for (int j = 0; j < 6; j++)
            {

                infoArr[i][j] = getValue(tempStr, '@', j);
            } // value
        }
    }
    // 数组赋值完成后应该是这个样子
    // [[icon,key,value],[icon,key,value],[icon,key,value],[icon,key,value]]
}
//数据处理
String readTcp()
{
    String data = "";
    while (client.available() > 0)
    {
        data += char(client.read()); // 读取数据
    }
    return data;
}

// 字符串截取
String getValue(String data, char separator, int index)
{
    int found = 0;
    int strIndex[] = {0, -1};
    int maxIndex = data.length() - 1;

    for (int i = 0; i <= maxIndex && found <= index; i++)
    {
        if (data.charAt(i) == separator || i == maxIndex)
        {
            found++;
            strIndex[0] = strIndex[1] + 1;
            strIndex[1] = (i == maxIndex) ? i + 1 : i;
        }
    }
    return found > index ? data.substring(strIndex[0], strIndex[1]) : "";
}
